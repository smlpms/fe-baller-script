-- hat needed: https://www.roblox.com/catalog/6685365462/Red-Stickman-Head
-- made by scriptifer#9511
pcall(function()
	settings().Physics.AllowSleep = false
	settings().Physics.PhysicsEnvironmentalThrottle = Enum.EnviromentalPhysicsThrottle.Disabled
end)
local Rig = loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Lowerrated/immortal/main/main"))()
local Debris = game:GetService("Debris")
local TweenService = game:GetService("TweenService")
local Ball = Instance.new("Part")
local A0Ball = Instance.new("Attachment")
local APBall = Instance.new("AlignPosition")
local A0ThrowArm = Instance.new("Attachment")
local ShowFling = true
local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
local threw = false
local stopped_walk = true
local walk_animation_timings = {
	[1] = 0,
	[2] = 0.03333333507180214,
	[3] = 0.10000000149011612,
	[4] = 0.13300000131130219,
	[5] = 0.20000000298023224,
	[6] = 0.23333333432674408,
	[7] = 0.30000001192092896,
	[8] = 0.3330000042915344,
	[9] = 0.36666667461395264,
	[10] = 0.4000000059604645,
	[11] = 0.4333333373069763,
	[12] = 0.46666666865348816,
	[13] = 0.5333333611488342,
	[14] = 0.5666666626930237,
	[15] = 0.6333333253860474,
	[16] = 0.6666666865348816,
	[17] = 0.7333333492279053,
	[18] = 0.800000011920929,
	[19] = 0.699999988079071,
}
for i = 2, 19 do
	walk_animation_timings[i] = walk_animation_timings[i]/30
end
local function findfirstdescendant(Object, Name)
	local DescendantOnFound
	for _, v in pairs(Object:GetDescendants()) do
		if v:IsA('Motor6D') then
			if v.Name == Name then
				DescendantOnFound = v
			end
		end
	end
	return DescendantOnFound
end
local function pose(_rig, t, num)
	for i, v in pairs(t) do
		local motor6d = findfirstdescendant(_rig, i)
		if motor6d then
			if num then
				TweenService:Create(motor6d, TweenInfo.new(num, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0.5, false, 0), {C1 = v.C1, C0 = v.C0}):Play()
			else
				motor6d.C0 = v.C0
				motor6d.C1 = v.C1
			end
		else
			spawn(function()
				error("motor 6D not found")
			end)
		end
	end
end
Rig.Animate.walk.WalkAnim.AnimationId = "rbxassetid://0"
Rig.Animate.run.RunAnim.AnimationId = "rbxassetid://0"
Rig.Animate.jump.JumpAnim.AnimationId = "rbxassetid://0"
Rig.Animate.fall.FallAnim.AnimationId = "rbxassetid://0"
local normal_joints = {}
for _, v in pairs(Rig:GetDescendants()) do
	if v:IsA("Motor6D") then
		normal_joints[v.Name] = {
			C0 = v.C0,
			C1 = v.C1
		}
	end
end
spawn(function()
	while true do
		if not stopped_walk then
			pose(Rig, animation_walk[1])
		end
		task.wait(walk_animation_timings[2])
		if not stopped_walk then
			pose(Rig, animation_walk[2])
		end
		task.wait(walk_animation_timings[3])
		if not stopped_walk then
			pose(Rig, animation_walk[3])
		end
		task.wait(walk_animation_timings[4])
		if not stopped_walk then
			pose(Rig, animation_walk[4])
		end
		task.wait(walk_animation_timings[5])
		if not stopped_walk then
			pose(Rig, animation_walk[5])
		end
		task.wait(walk_animation_timings[6])
		if not stopped_walk then
			pose(Rig, animation_walk[6])
		end
		task.wait(walk_animation_timings[7])
		if not stopped_walk then
			pose(Rig, animation_walk[7])
		end
		task.wait(walk_animation_timings[8])
		if not stopped_walk then
			pose(Rig, animation_walk[8])
		end
		task.wait(walk_animation_timings[9])
		if not stopped_walk then
			pose(Rig, animation_walk[9])
		end
		task.wait(walk_animation_timings[10])
		if not stopped_walk then
			pose(Rig, animation_walk[10])
		end
		task.wait(walk_animation_timings[11])
		if not stopped_walk then
			pose(Rig, animation_walk[11])
		end
		task.wait(walk_animation_timings[12])
		if not stopped_walk then
			pose(Rig, animation_walk[12])
		end
		task.wait(walk_animation_timings[13])
		if not stopped_walk then
			pose(Rig, animation_walk[13])
		end
		task.wait(walk_animation_timings[14])
		if not stopped_walk then
			pose(Rig, animation_walk[14])
		end
		task.wait(walk_animation_timings[15])
		if not stopped_walk then
			pose(Rig, animation_walk[15])
		end
		task.wait(walk_animation_timings[16])
		if not stopped_walk then
			pose(Rig, animation_walk[16])
		end
		task.wait(walk_animation_timings[17])
		if not stopped_walk then
			pose(Rig, animation_walk[17])
		end
		task.wait(walk_animation_timings[18])
		if not stopped_walk then
			pose(Rig, animation_walk[18])
		end
		task.wait(walk_animation_timings[19])
		if not stopped_walk then
			pose(Rig, animation_walk[19])
		end
	end
end)
local walk_animation_editor = {
	Play = function()
		stopped_walk = false
	end,
	Stop = function()
		stopped_walk = true
		for _, v in pairs(Rig:GetDescendants()) do
			if v:IsA("Motor6D") then
				v.C0 = normal_joints[v.Name].C0
				v.C1 = normal_joints[v.Name].C1
			end
		end
	end,
}
local RigHum = Rig.Humanoid
spawn(function()
	RigHum.Running:Wait()
	RigHum.Running:Connect(function(speed)
		if not RigHum.Jump then
			if speed > 0 then
				walk_animation_editor.Play()
			else
				walk_animation_editor.Stop()
			end
		end
	end)
	RigHum:GetPropertyChangedSignal("FloorMaterial"):Connect(function()
		task.wait(0.1)
		if RigHum.MoveDirection.Magnitude > 0 then
			walk_animation_editor.Play()
		else
			walk_animation_editor.Stop()
		end
	end)
end)
for i, v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
	if not v:IsDescendantOf(Rig) then
		if v:IsA("BasePart") then 
			spawn(function()
				while true do
					v.Velocity = Vector3.new(45,0,0)
					task.wait(0)
				end
			end)
		end
	end
end
A0ThrowArm.Parent = Rig["Right Arm"]
A0ThrowArm.Position = Vector3.new(0, -1.7, 0)
Ball.Parent = workspace
Ball.Shape = Enum.PartType.Ball
Ball.Size = Vector3.new(1.65, 1.65, 1.65)
Ball.Transparency = 1
Ball.Name = "Baller_Ball"
A0Ball.Parent = Ball
APBall.Parent = Ball
APBall.Attachment0 = A0Ball
APBall.Attachment1 = A0ThrowArm
APBall.Responsiveness = 200
APBall.MaxForce = math.huge
Ball.CanCollide = false
local function get(part)
	if not APBall.Enabled then
		if part then
			if part:IsDescendantOf(Rig.Parent) then
				threw = false
				Ball.CanCollide = false
				APBall.Enabled = true
			end
		else
			threw = false
			Ball.CanCollide = false
			APBall.Enabled = true
		end
	end
end
local function throw()
	if not threw then
		threw = true
		if RigHum.MoveDirection.Magnitude == 0 then
			for _, v in pairs(RigHum:GetPlayingAnimationTracks()) do
				repeat task.wait(0) v:Stop() until v.IsPlaying == false
			end
			Rig.Animate.Disabled = true
			pose(Rig, animation_throw[1])
			task.wait(0.233)
			pose(Rig, animation_throw[2], 0.3)
			task.wait(0.3)
			pose(Rig, animation_throw[3])
			task.wait()
			pose(Rig, animation_throw[4])
			task.wait()
			pose(Rig, animation_throw[5], 0.367)
			APBall.Enabled = false
			Ball.CanCollide = true
			Ball.Velocity = Mouse.hit.LookVector*250
			task.wait(0.367)
			walk_animation_editor.Stop()
			Rig.Animate.Disabled = false
		end
	end
end
pcall(function()
	Rig["RedStickman"]:Destroy()
end)
spawn(function()
	local accessory = Rig.Parent["RedStickman"].Handle
	local a0accessory = Instance.new("Attachment")
	local apaccessory = Instance.new("AlignPosition")
	a0accessory.Parent = accessory
	apaccessory.Parent = accessory
	apaccessory.Attachment0 = a0accessory
	apaccessory.Attachment1 = A0Ball
	apaccessory.Responsiveness = 200
	apaccessory.MaxForce = math.huge
	if getsynasset and isfile and writefile then
		local BoomSound = Instance.new("Sound")
		BoomSound.Parent = accessory
		local file_exists = isfile("boom.mp3")
		if not file_exists then
			writefile("boom.mp3", game:HttpGetAsync("https://github.com/Lowerrated/Lowerrated/blob/main/boom.mp3?raw=true"))
		end
		BoomSound.SoundId = getsynasset("boom.mp3")
		Rig.Parent.HumanoidRootPart.Touched:Connect(function(part)
			if not part:IsDescendantOf(Rig.Parent) then
				if part.Parent:FindFirstChildOfClass("Humanoid") then
					BoomSound.Playing = true
				end
			end
		end)
	end
end)
local fling = Instance.new("BodyAngularVelocity")
local apfling = Instance.new("AlignPosition")
local a0fling = Instance.new("Attachment")
if ShowFling then
	local selection = Instance.new("SelectionBox")
	selection.Parent = workspace
	selection.Adornee = Rig.Parent.HumanoidRootPart
end
Rig.Parent.HumanoidRootPart.AlignOrientation:Destroy()
fling.Parent = Rig.Parent.HumanoidRootPart
fling.AngularVelocity = Vector3.new(1000, 1000, 1000)
apfling.Parent = Rig.Parent.HumanoidRootPart
a0fling.Parent = apfling.Parent
apfling.Attachment0 = a0fling
apfling.Attachment1 = A0Ball
apfling.Responsiveness = 200
apfling.MaxForce = math.huge
Mouse.Button1Down:Connect(function()
	throw()
end)
Mouse.KeyDown:Connect(function(key)
	if string.lower(key) == "e" then
		if threw then
			get()
		end
	end
end)
UIS.InputBegan:Connect(function(io, gpe)
	if gpe then
		if io.KeyCode == Enum.KeyCode.LeftShift then
			for i = 2, 19 do
				walk_animation_timings[i] = walk_animation_timings[i]/2
			end
			RigHum.WalkSpeed = 32
		end
	end
end)
UIS.InputEnded:Connect(function(io, gpe)
	if gpe then
		if io.KeyCode == Enum.KeyCode.LeftShift then
			for i = 2, 19 do
				walk_animation_timings[i] = walk_animation_timings[i]*2
			end
			RigHum.WalkSpeed = 16
		end
	end
end)
